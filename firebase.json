{
  "hosting": {
    "public": "no",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  },
  "storage": {
    "rules": "storage.rules",
    "cors": [
      {
        "origin": [
          "*"
        ]
      }
    ]
  }
}